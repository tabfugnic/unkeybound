{
  "manifest_version": 2,
  "name": "Unkeybound",
  "version": "0.1.0",
  "description": "Unbind custom keybindings on the web to manage surprises",

  "icons": {
    "48": "icons/key-48.png",
    "96": "icons/key-96.png"
  },

  "content_scripts": [
    {
      "matches": ["*://*.github.com/*"],
      "js": ["unkeybound.js"]
    }
  ]

}
